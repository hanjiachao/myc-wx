/**index.wxss**/
.opus-item{
  margin-bottom: 15px;
}
.opus-item>image{
  width: 100%;
  height: 240px;
}
.userinfo {
  display: flex;
  flex-direction: column;
  align-items: center;
}
.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userinfo-nickname {
  color: #aaa;
}
.notext{
  padding: 40px 0px;
  text-align: center;
  color: #cacaca;
}
.usermotto {
  margin-top: 200px;
}
.header-personal {
  height: 280px;
  padding: 20px 15px;
  background-repeat: no-repeat;
  background-image:url('data:image/png;base64,');
  background-size: cover;
  background-position: center;
}
.search {
  background: #fff;
  border-radius: 15px;
  padding: 5px 15px;
  overflow: hidden;
}
.search-input image {
  width: 21px;
  margin-top: 1px;
  margin-right: 10px;
  height: 21px;
}
.fl {
  float: left;
}
.fr {
  float: right;
}
.search-input input {
  width: calc(100% - 80px);
  outline: none;
}
.content-box {
  padding: 15px;
  overflow: hidden;
}
.banner {
  margin-top: -265px;
}
.navbar-item {
  width: calc(50% - 5px);
  height: 91px;
  overflow: hidden;
  box-shadow: 3px 4px 10px #e0e0e0;
  margin-right: 5px;
  border-radius: 5px;
}
.navbar-item:last-child{
  margin-left: 5px;
  margin-right: 0px;
}
.navbar-item:first-child .nav-title:after{
  bottom: 28px;
}
.navbar-item image {
  width: 100px;
  height:91px;
  border-radius: 5px 0px 0px 5px;
}
.nav-title {
  width:calc(100% - 100px);
  float: right;
  position: relative;
  font-weight: 500;
  text-align: center;
}
.nav-title view{
  width:34px;
  margin: 20px auto 0px;
}
.nav-title:after {
  content: '';
  position: absolute;
  width: 34px;
  background: #FDC31A;
  height: 3px;
  bottom: 0px;
  left: 50%;
  margin-left: -17px;
}
.bacyellow{
  background: #FDC31A!important;
}
.star {
  padding: 28px 0px;
  overflow: hidden;
  background: #fff;
  margin: 15px 0px;
}
.star-item {
  width: 33.33%;
  float: left;
  text-align: center;
  position: sticky;
  padding: 20px 0px 0px;
}
.star-item image {

}
.star-name .star-img {
  background: url('data:image/png;base64,') no-repeat;
  background-size: cover;
  width: 20px;
  height: 23px;
  text-align: center;
  color: #fff;
  display: inline-block;
  background-position: center;
  margin-right: 6px;
  line-height: 29px;
  font-size: 14px;
}
.star-item:nth-child(2) .star-img{
  background: url('data:image/png;base64,') no-repeat;
  background-size: cover;
  background-position: center;
}
.star-item:nth-child(3) .star-img{
  background: url('data:image/png;base64,') no-repeat;
  background-size: cover;
  background-position: center;
}
.star-item:nth-child(2) {
  margin-top: -17px;
}
.star-img1{

}
.select {
background: #F5F6FB;
padding: 8px;
overflow: hidden;
margin: 15px 0px;
height: 30px;
}
.select-item {
  padding: 3px 10px;
  border-radius: 15px;
  /* background: #FDC31A; */
  margin-right: 15px;
  font-size: 14px;
}
.opus-t1 {
  margin: 5px 0px;
  font-size: 18px;
}
.opus-t2 {
  color: #A1A2A2;
  font-size: 14px;
  line-height: 22px;
}
.opus-t3 {
  color: #5A5A5A;
  font-size: 14px;
  margin-top: 3px;
}
.opus-t3 image {
  width: 20px;
  height:16px;
  margin-right: 8px;
  vertical-align: -2px;
}
.navbar-item button{
  width:100%;
  height: 91px;
  padding: 0px;
  margin: 0px;
  display: block;
  border:0px;
  min-height: 0;
  box-sizing: content-box;
  padding-left: 0px;
  padding-right: 0px;
  border-radius: 0px;
  font-size: 16px;
  border-radius: 5px;
}
swiper{
  height: 180px!important;
}
swiper-item {
  border-radius: 5px;
}
.opus-item>image{
  width:100%;
  border-radius: 10px;
}
.bac{
  width:100%;
  height: 100vh;
  background: #F5F6FB;
  position: fixed;
  left:0px;
  top:0px;
  z-index:0;
}
.search-btn{
  color: #fdc31a;
}
.pho-img{
  width: 60px;
  text-align: center;
  height: 60px;
  border-radius: 50%;
  margin-top: -5px;
}
.top-img{
  width: 42px;
  height: 34px;
  margin: 0 auto;
  display: block;
  border-radius: 0px;
}