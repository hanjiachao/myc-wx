.content-box {
  padding: 15px;
  overflow: hidden;
}
.select {
  background: #F5F6FB;
  padding: 8px;
  overflow: hidden;
  margin: 0px;
  height: 30px;
}
.select-item {
  padding: 3px 10px;
  border-radius: 15px;
  margin-right: 15px;
  font-size: 14px;
}
.bacyellow{
  background: #FDC31A!important;
}
.label{
  display: inline-block;
  width: 22px;
  height: 26px;
  background: url('data: image/png; base64,') no-repeat;
  background-size: cover;
  background-position: center;
  font-size: 12px;
  color: #fff;
  line-height: 33px;
  text-align: center;

}
.fl {
  float: left;
}
.select-item>image {
  width: 8px;
  vertical-align: 3px;
  height: 7px;
}
.opus-item {
  margin-top: 10px;
}
.opus-t1 {
  margin: 5px 0px;
  font-size: 18px;
}
.opus-t2 {
  color: #A1A2A2;
  font-size: 14px;
  line-height: 22px;
}
.opus-t3 image {
  width: 20px;
  height: 16px;
  margin-right: 8px;
  vertical-align: -2px;
}
.noText{
  text-align: center;
  padding: 40px 0px;
  color: #cacaca;
}
.opus-item>image{
  width:100%;
  border-radius: 10px;
}